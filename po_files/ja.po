#: src/views/Dashboard/SupplyChainDashboardKpi.js
#, fuzzy
#| msgid ""
#| "Calls are assigned levels from 1-4 to denote the severity of the "
#| "situation. All information queries and communications around coordination "
#| "are categorized as Level 1, the least severe level. Level 2 cases are "
#| "reported labour violations that do not violate national law but violate "
